{
  "name": "react-express-starter-kit",
  "version": "1.0.0",
  "description": "Boiler plate for react express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexyking/React-Express-Starter-Pack.git"
  },
  "author": "Joan Yassi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lexyking/React-Express-Starter-Pack/issues"
  },
  "homepage": "https://github.com/lexyking/React-Express-Starter-Pack#readme",
  "dependencies": {
    "concurrently": "^4.1.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}